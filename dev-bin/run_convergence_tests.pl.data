                              $aref->[$i], 0,
                              $RootWidth
                      ) eq substr(
                               $aref->[ $i + 1 ],
                               0,
                               $RootWidth
                      )
                    )
                    ? "Instability"
                    : "Disorder";
        }
}
__END__

sub checkorder{
        my ($i,
              $disorder);

        for ($i = 0;
              $i < $#$aref;
              ++$i)
        {
# Equality shouldn't happen, but catch it in the contents check

                $disorder =
                    (
                      substr(
                              $aref->[$i], 0,
                              $RootWidth
                      ) eq substr(
                               $aref->[ $i + 1 ],
                               0,
                               $RootWidth
                      )
                    )
                    ? "Instability"
                    : "Disorder";
        }
}

