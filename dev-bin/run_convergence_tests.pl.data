            my $self = shift->new;
            die SOAP::Fault->faultcode('Server.RequestError')
              ->faultstring('Could not get object')
              unless $self;

            my $smethod = "SUPER::$method";
            my $res     = $self->$smethod(@_);
            die SOAP::Fault->faultcode('Server.ExecError')
              ->faultstring("Execution error: $res")
              unless ref($res);

            $res;
        };
      }
